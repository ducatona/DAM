Algoritmo ejercicio6
	Definir num1,num2 Como Real
	
	Escribir "Dime el primer numero real";
	Leer num1;

	Escribir "Dime el segundo numero real";
	Leer num2;
	
	Si num1<num2 Entonces
		Escribir "El segundo numero es mayor"
		Si num1>num2 Entonces
			Escribir "El primer numero es mayor"
		Fin Si
	SiNo
		Escribir "Los dos son iguales";
	Fin Si

FinAlgoritmo
