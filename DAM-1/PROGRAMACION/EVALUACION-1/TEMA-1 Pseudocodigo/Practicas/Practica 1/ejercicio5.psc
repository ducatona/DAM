Algoritmo ejercicio5
	Definir radio,altura,volumen Como Real
	
	Mientras radio<=0 Hacer
		Escribir "Dime el radio del circulo";
		Leer radio;
	Fin Mientras
	
	Mientras altura<=0 Hacer
		Escribir "Dime el radio del circulo";
		Leer altura;
	Fin Mientras
	
	volumen=2*PI*radio*altura;
	
	Escribir "El volumen del cilindro es: " volumen;
	
FinAlgoritmo
